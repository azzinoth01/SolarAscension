    margin-bottom: 0;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 0;
    padding-bottom: 0;
}

.resource-label {
    height: 80%;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-left;
    font-size: 15px;
    width: 65%;
    margin-left: 5px;
    -unity-font-style: bold;
    margin-right: 5px;
    -unity-font: url('project://database/Assets/01_Art/Font/GratiaSans/GratiaSans-Regular.ttf?fileID=12800000&guid=6de7e15782c85134890019d82a150567&type=3#GratiaSans-Regular');
