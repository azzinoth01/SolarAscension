}

.module-selector {
    height: 70%;
    width: 6%;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 4px;
    margin-bottom: 4px;
    font-size: 15px;
    border-top-left-radius: 15px;
    border-bottom-left-radius: 15px;
    border-top-right-radius: 15px;
    border-bottom-right-radius: 15px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    color: rgba(27, 27, 27, 0);
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    -unity-background-scale-mode: scale-to-fit;
}

.module-selector:hover {
    -unity-background-image-tint-color: rgb(255, 200, 150);
}

.tier-selector {
    position: relative;
    width: 15%;
    height: 90%;
    font-size: 20px;
    background-image: url('project://database/Assets/01_Art/UI/Ingame/BuildMenu/BuildMenu_TierSelection.png?fileID=21300000&guid=219fb8b4e733c7a46bdd3dd69ef0c64d&type=3#BuildMenu_TierSelection');
    -unity-background-scale-mode: scale-to-fit;
    -unity-background-image-tint-color: rgb(219, 216, 216);
    opacity: 1;
    -unity-font: url('project://database/Assets/01_Art/Font/GratiaSans/GratiaSans-Regular.ttf?fileID=12800000&guid=6de7e15782c85134890019d82a150567&type=3#GratiaSans-Regular');
    -unity-font-definition: url('project://database/Assets/01_Art/Font/GratiaSans/GratiaSans-Regular%20SDF.asset?fileID=11400000&guid=207c5ad8a9b5df14ba5330972a05bf0c&type=2#GratiaSans-Regular SDF');
    margin-left: 2px;
    margin-right: 2px;
}

.tier-selector:hover {
    -unity-background-image-tint-color: rgb(224, 181, 102);
}

.tier-selector-selected {
    -unity-background-image-tint-color: rgb(224, 181, 102);
}

.resource-display {
    height: 100%;
