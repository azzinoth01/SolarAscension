    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-image: url('project://database/Assets/01_Art/UI/Ingame/ModulePopup/Delete.png?fileID=21300000&guid=d48062a65d1dd9649b9e2a0143611a4b&type=3#Delete');
